<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>FEA | Home</title>
    <script src="https://code.jquery.com/jquery-2.1.3.min.js"></script>
    <!-- jQuery 2.2.3 (Botoes do menus efeito de abrir e fechar)-->
    <script src="@Url.Content("~/Scripts/jQuery/jquery-2.2.3.min.js")"></script>
    <!-- jQuery UI 1.11.4 -->
    <!-- Responsividade-->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.6 -->
    <link rel="stylesheet" href="~/Content/bootstrap.min.css" />
    <!--  Font Awesomw  -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
    <!-- Biblioteca Framework Ionicons -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
    <link rel="stylesheet" href="~/Content/CSS/ionicons.min.css">
    <link rel="stylesheet" href="~/Content/CSS/ionicons.css">
    <!-- TEMAS DA PAGINA -->
    <link rel="stylesheet" href="~/Content/CSS/FEA.min.css" />
    <link rel="stylesheet" href="~/Content/CSS/FEA.css" />
    <link rel="stylesheet" href="~/Content/CSS/skin/_all-skins.min.css">
    <!-- iCheck - O modelos dos Chekbox-->
    <link rel="stylesheet" href="~/Content/Plugins/Check/square/blue.css" />
    <!-- Date Picker -->
    <link rel="stylesheet" href="~/Content/Plugins/DataPicker/datepicker3.css" />
    <!-- Daterange picker -->
    <link rel="stylesheet" href="~/Content/Plugins/DataPicker/DataRangePicker/daterangepicker.css" />
    <!-- bootstrap wysihtml5 - text editor -->
    <link rel="stylesheet" href="~/Content/Plugins/Bootstrap-wysihtml5/bootstrap3-wysihtml5.css" />
    <!--  App Alert SweetAlert -->
    <script src="@Url.Content("~/Scripts/sweetalert/dist/sweetalert.min.js")"></script>
    <link rel="stylesheet" type="text/css" href="~/Scripts/sweetalert/dist/sweetalert.css">
    
    <script src="@Url.Content("~/Scripts/AppMensagem.js")"></script>
</head>
<body class="corpo_pagina skin-blue sidebar-mini">
    <div class="wrapper">

        <header class="main-header">
            <!-- Logo -->
            <a href="@Url.Action("Index", "Home")" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="Foto-User"><b>F</b>EA</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>Professor</b>FEA</span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Menu</span>
                </a>
                <!-- BARRA SUPERIOR -->
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                       
                    <!-- Barra superior -->
                    <li class="dropdown user user-menu">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                            <img src="~/Content/Img/user2-160x160.jpg" class="user-image" alt="User Image">
                            <span class="hidden-xs">Professor</span>
                        </a>
                        <ul class="dropdown-menu">
                            <!-- User image -->
                            <li class="user-header">
                                <img src="~/Content/Img/user2-160x160.jpg" class="img-circle" alt="User Image">

                                <p>
                                    Professor
                                    <small>Menbro desde de Nov. 2012</small>
                                </p>
                            </li>
                            <!-- Menu Body -->
                            <li class="user-body">
                                <div class="row">
                                    <div class="col-xs-4 text-center">
                                        <a href="#">Seguidores</a>
                                    </div>
                                    <div class="col-xs-4 text-center">
                                        <a href="#">Seguindo</a>
                                    </div>
                                    <div class="col-xs-4 text-center">
                                        <a href="#">Amigos</a>
                                    </div>
                                </div>
                                <!-- /.row -->
                            </li>
                            <!-- Menu Footer-->
                            <li class="user-footer">
                                <div class="pull-left">
                                    <a href="#" class="btn btn-default btn-flat">Perfil</a>
                                </div>
                                <div class="pull-right">
                                    <a href="login2.html" class="btn btn-default btn-flat">Logout out</a>
                                </div>
                            </li>
                        </ul>
                    </li>
                    <!-- Control Sidebar Toggle Button -->
                    <li>
                        <a href="#" data-toggle="control-sidebar"><i class="fa fa-gears"></i></a>
                    </li>
                    </ul>
                </div>
            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel -->
                <div class="user-panel">
                    <div class="pull-left image">
                        <img src="~/Content/Img/user2-160x160.jpg" class="img-circle" alt="User Image">
                    </div>
                    <div class="pull-left info">
                        <p>Professor</p>
                        <a href="#"><i class="fa fa-circle text-success"></i> Online</a>
                    </div>
                </div>
                <!-- search form -->
                <form action="#" method="get" class="sidebar-form">
                    <div class="input-group">
                        <input type="text" name="q" class="form-control" placeholder="Search...">
                        <span class="input-group-btn">
                            <button type="submit" name="search" id="search-btn" class="btn btn-flat">
                                <i class="fa fa-search"></i>
                            </button>
                        </span>
                    </div>
                </form>
                <!-- /.search form -->
                <!-- sidebar menu: : style can be found in sidebar.less -->
                <!-- Barra Lateral-->
                <ul class="sidebar-menu">
                    <li class="header">Menu</li>
                    <li>
                        <a href="@Url.Action("Index", "Home")">
                            <i class="fa fa-dashboard"></i> <span>Home</span>
                        </a>
                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-table"></i> <span>Listas</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="@Url.Action("Arvores" , "Arvore")"><i class="fa fa-circle-o"></i> Arvores </a></li>
                            <li><a href="@Url.Action("ArvoreAluno", "Arvore")"><i class="fa fa-circle-o"></i> Alunos </a></li>
                        </ul>
                    </li>
                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            @RenderBody()
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Versão</b>0.0.0.01_BETA
            </div>
            <strong>Copyright &copy; 2016 <a href="http://Univali.br"> Univali </a>.</strong> Alexsandro , Gustavo , Hélio
        </footer>

        <!-- Barra de configuração -->
        <aside class="control-sidebar control-sidebar-dark">
            <!-- Criaçao da Barra -->
            <ul class="nav nav-tabs nav-justified control-sidebar-tabs">
                <li><a href="#control-sidebar-home-tab" data-toggle="tab"><i class="fa fa-home"></i></a></li>
                <li><a href="#control-sidebar-settings-tab" data-toggle="tab"><i class="fa fa-gears"></i></a></li>
            </ul>
            <!-- Painel de Configuração -->
            <div class="tab-content">
                <!-- Painel Home -->
                <div class="tab-pane" id="control-sidebar-home-tab">
                    <h3 class="control-sidebar-heading">Notificações</h3>
                    <ul class="control-sidebar-menu">
                        <li>
                            <a href="javascript:void(0)">
                                <i class="menu-icon fa fa-envelope-o bg-light-blue"></i>

                                <div class="menu-info">
                                    <h4 class="control-sidebar-subheading">Você Recebeu um  E-mail</h4>

                                    <p>professor@exemplo.com</p>
                                </div>
                            </a>
                        </li>
                    </ul>
                </div>
                <!-- /.Painel Home -->
                <!-- Criação da Barra de Cores da pagina-->
                <div class="tab-pane" id="control-sidebar-stats-tab">Skill</div>
                <!-- /. Criação da Barra de Cores da pagina -->
                <!-- Configuração Geral -->
                <div class="tab-pane" id="control-sidebar-settings-tab">
                    <form method="post">
                        <h3 class="control-sidebar-heading">Configurações Geral</h3>
                        <!-- Primeira config-->
                        <div class="form-group">
                            <label class="control-sidebar-subheading">
                                Permitir redirecionamento de E-mail
                                <input type="checkbox" class="pull-right" checked>
                            </label>

                            <p>
                                Outros conjuntos de opções estão disponíveis
                            </p>
                        </div>
                        <!-- /.Primeira config -->
                        <!-- Segunda config -->
                        <div class="form-group">
                            <label class="control-sidebar-subheading">
                                Expor o nome do autor em post
                                <input type="checkbox" class="pull-right" checked>
                            </label>

                            <p>
                                Permitir mostrar o meu nome de usuário postagens.
                            </p>
                        </div>
                        <!-- /.Segunda config-->
                        <!-- /.Terceira config-->
                        <h3 class="control-sidebar-heading">Configuração Notificação</h3>
                        <div class="form-group">
                            <label class="control-sidebar-subheading">
                                Desativar Notificação
                                <input type="checkbox" class="pull-right">
                            </label>
                        </div>

                    </form>
                </div>
                <!-- /.tab-pane -->
            </div>
        </aside>
        <!-- /.control-sidebar -->
        <div class="control-sidebar-bg"></div>
    </div>
    <!-- ./wrapper -->
    <script>
  $.widget.bridge('uibutton', $.ui.button);
    </script>

    <!-- Bootstrap 3.3.6 -->
    <script src="~/Scripts/bootstrap.min.js"></script>
    <!-- Morris.js charts -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js"></script>
    <script src="@Url.Content("~/Scripts/morris/morris.min.js")"></script>
    <!-- Sparkline -->
    <script src="@Url.Content("~/Content/plugins/sparkline/jquery.sparkline.min.js")"></script>
    <!-- jQuery Knob Chart -->
    <script src="@Url.Content("~/Content/plugins/knob/jquery.knob.js")"></script>
    <!-- daterangepicker -->
    @*<script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.11.2/moment.min.js"></script>*@

    <script src="@Url.Content("~/Content/plugins/datapicker/datarangepicker/moment.min.js")"></script>
    <script src="@Url.Content("~/Content/plugins/datapicker/datarangepicker/daterangepicker.js")"></script>
    <!-- datepicker -->
    <script src="@Url.Content("~/Content/plugins/datapicker/bootstrap-datepicker.js")"></script>
    <!-- Bootstrap WYSIHTML5 -->
    <script src="@Url.Content("~/content/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js")"></script>
    <!-- Slimscroll -->
    <script src="@Url.Content("~/Content/plugins/slimScroll/jquery.slimscroll.min.js")"></script>
    <!-- FastClick -->
    <script src="@Url.Content("~/Content/plugins/fastclick/fastclick.js")"></script>
    <!-- App -->
    <script src="~/Scripts/Pages/app.js"></script>
    <script src="~/Scripts/Pages/dashboard.js"></script>
    <script src="@Url.Content("~/Scripts/Pages/demo.js")"></script>
</body>
</html>
